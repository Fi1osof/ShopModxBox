{
  "name": "starter-app",
  "version": "0.0.1",
  "description": "Starter App",
  "engines": {
    "npm": ">=3"
  },
  "author": "",
  "license": "MIT",
  "scripts": {
    "prebuild": "npm run build:clean",
    "build": "cross-env NODE_ENV=production webpack --config internals/webpack/webpack.prod.babel.js --color -p --progress",
    "build:clean": "rimraf ./build",
    "start": "cross-env DEBUG=react-cms:* PORT=3000 NODE_ENV=development node app/modules/Site/server --color",
    "start:production": "npm run build && npm run start:prod",
    "start:dev": "cross-env PORT=3000 DEBUG=react-cms:* NODE_ENV=development pm2 start app/modules/Site/server --node-args='--max-old-space-size=1000' --name shopmodx-dev",
    "start:prod": "cross-env PORT=3000 NODE_ENV=production node app/modules/Site/server --name shopmodx",
    "start:forever": "cross-env PORT=3000 NODE_ENV=production pm2 start app/modules/Site/server --name shopmodx-prod",
    "lint": "npm run lint:js",
    "lint:eslint": "eslint --ignore-path .stylelintignore",
    "lint:js": "npm run lint:eslint -- . ",
    "lint:staged": "lint-staged",
    "test": "cross-env NODE_ENV=test jest --coverage",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll"
  },
  "lint-staged": {
    "*.js": "lint:eslint"
  },
  "pre-commit": "lint:staged",
  "babel": {
    "presets": [
      [
        "es2015",
        {
          "modules": false
        }
      ],
      "react",
      "stage-0"
    ],
    "env": {
      "production": {
        "only": [
          "app"
        ],
        "plugins": [
          "transform-react-remove-prop-types",
          "transform-react-constant-elements",
          "transform-react-inline-elements"
        ]
      }
    }
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "airbnb",
    "env": {
      "browser": true,
      "node": true,
      "mocha": true,
      "es6": true
    },
    "plugins": [
      "redux-saga",
      "react",
      "jsx-a11y"
    ],
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "rules": {
      "arrow-parens": [
        "error",
        "always"
      ],
      "arrow-body-style": [
        2,
        "as-needed"
      ],
      "comma-dangle": [
        2,
        "always-multiline"
      ],
      "import/imports-first": 0,
      "import/newline-after-import": 0,
      "import/no-dynamic-require": 0,
      "import/no-extraneous-dependencies": 0,
      "import/no-named-as-default": 0,
      "import/no-unresolved": 2,
      "import/prefer-default-export": 0,
      "indent": [
        1,
        4,
        {
          "SwitchCase": 1
        }
      ],
      "jsx-a11y/aria-props": 2,
      "jsx-a11y/heading-has-content": 0,
      "jsx-a11y/href-no-hash": 2,
      "jsx-a11y/label-has-for": 2,
      "jsx-a11y/mouse-events-have-key-events": 2,
      "jsx-a11y/role-has-required-aria-props": 2,
      "jsx-a11y/role-supports-aria-props": 2,
      "max-len": 0,
      "newline-per-chained-call": 0,
      "no-console": 1,
      "no-tabs": 0,
      "no-use-before-define": 0,
      "prefer-template": 2,
      "class-methods-use-this": 0,
      "react/forbid-prop-types": 0,
      "react/jsx-first-prop-new-line": [
        2,
        "multiline"
      ],
      "react/jsx-filename-extension": 0,
      "react/jsx-no-target-blank": 0,
      "react/require-extension": 0,
      "react/self-closing-comp": 0,
      "react/jsx-indent": [
        1,
        4
      ],
      "react/jsx-indent-props": [
        0
      ],
      "redux-saga/no-yield-in-race": 2,
      "redux-saga/yield-effects": 2,
      "require-yield": 0
    },
    "settings": {
      "import/resolver": {
        "webpack": {
          "config": "./internals/webpack/webpack.test.babel.js"
        }
      }
    }
  },
  "dependencies": {
    "babel-polyfill": "6.22.0",
    "chalk": "1.1.3",
    "compression": "1.6.2",
    "cross-env": "3.1.3",
    "draft-js": "^0.10.1",
    "draft-js-export-html": "^1.2.0",
    "expect": "1.20.2",
    "google-map-react": "^0.24.0",
    "google-map-react-control": "^1.0.1",
    "google-maps": "^3.2.1",
    "graphiql": "^0.11.3",
    "graphql-type-json": "^0.1.4",
    "history": "^3.3.0",
    "immutable": "^3.8.1",
    "ip": "1.1.3",
    "jquery": "^1.12.4",
    "js-cookie": "^2.2.0",
    "konva": "^1.7.6",
    "locale": "^0.1.0",
    "lodash": "^4.17.4",
    "material-ui": "^1.0.0-alpha.16",
    "material-ui-components": "^1.14.12",
    "material-ui-icons": "1.0.0-alpha.3",
    "md5": "^2.2.1",
    "minimist": "1.2.0",
    "moment": "^2.19.3",
    "mysql": "^2.14.1",
    "node-sass": "^4.5.3",
    "objection": "^0.8.8",
    "prop-types": "15.5.8",
    "react": "^15.6.2",
    "react-big-calendar": "^0.17.0",
    "react-cms": "^1.4.0",
    "react-cms-data-view": "^1.4.6",
    "react-cms-draft-wysiwyg": "^1.12.0",
    "react-color": "^2.13.0",
    "react-decliner": "^1.0.0",
    "react-dnd": "^2.4.0",
    "react-dnd-html5-backend": "^2.4.1",
    "react-dom": "^15.6.2",
    "react-draft-wysiwyg": "^1.10.12",
    "react-image-crop": "^2.0.5",
    "react-images-uploader": "^1.1.0",
    "react-konva": "^1.1.3",
    "react-motion": "^0.5.0",
    "react-number-format": "^3.1.0",
    "react-redux": "5.0.1",
    "react-router": "3.0.0",
    "react-router-scroll": "0.3.3",
    "react-schedule": "^1.2.1",
    "react-slick": "^0.15.4",
    "react-tap-event-plugin": "^2.0.1",
    "react-yandex-maps": "^2.0.0",
    "rebass": "0.3.4",
    "redux": "3.6.0",
    "redux-saga": "0.12.0",
    "reselect": "2.5.4",
    "sass-loader": "^6.0.6",
    "shopmodx-react": "^1.0.0",
    "slick-carousel": "^1.8.1",
    "structor-templates": "^1.3.0",
    "supercluster": "^2.3.0",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "autoprefixer-loader": "^3.2.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.2.1",
    "babel-loader": "^6.4.1",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es3-member-expression-literals": "^6.22.0",
    "babel-plugin-transform-es3-property-literals": "^6.22.0",
    "babel-plugin-transform-react-constant-elements": "6.9.1",
    "babel-plugin-transform-react-inline-elements": "6.8.0",
    "babel-plugin-transform-react-remove-prop-types": "0.2.10",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-preset-stage-2": "^6.24.1",
    "body-parser": "^1.18.2",
    "canvas": "^1.6.7",
    "cheerio": "^0.22.0",
    "cookie-parser": "^1.4.3",
    "css-loader": "0.25.0",
    "eslint": "3.9.0",
    "eslint-config-airbnb": "12.0.0",
    "eslint-config-airbnb-base": "9.0.0",
    "eslint-import-resolver-webpack": "0.6.0",
    "eslint-plugin-import": "^2.6.0",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "6.4.1",
    "eslint-plugin-redux-saga": "0.1.5",
    "eventsource-polyfill": "0.9.6",
    "exports-loader": "0.6.3",
    "express": "4.14.0",
    "express-ws": "^3.0.0",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "0.9.0",
    "geoip-lite": "^1.2.1",
    "html-loader": "0.4.4",
    "html-webpack-plugin": "2.24.0",
    "image-webpack-loader": "2.0.0",
    "imports-loader": "0.6.5",
    "jest": "^20.0.4",
    "jest-cli": "^20.0.4",
    "jsdom": "^11.3.0",
    "json-loader": "0.5.4",
    "knex": "^0.13.0",
    "less": "^2.7.2",
    "less-loader": "^4.0.4",
    "lint-staged": "3.2.0",
    "node-gyp": "^3.6.2",
    "postcss-loader": "^2.0.6",
    "pre-commit": "1.1.3",
    "react-hot-loader": "^3.0.0-beta.6",
    "redux-devtools": "^3.4.0",
    "redux-devtools-dock-monitor": "^1.1.2",
    "redux-devtools-log-monitor": "^1.3.0",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "svg-react-loader": "^0.4.5",
    "url-loader": "0.5.7",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.10.0",
    "webpack-hot-middleware": "^2.16.0"
  }
}
